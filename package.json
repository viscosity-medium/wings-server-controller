{
  "name": "wings-server-controller",
  "version": "1.0.0",
  "description": "",
  "main": "master-server.ts",
  "scripts": {
    "dev:office": "cross-env NODE_MODE=development TEST_LOCATION=office nodemon -r dotenv/config master-server.ts",
    "dev:museum": "cross-env NODE_MODE=development TEST_LOCATION=museum nodemon -r dotenv/config master-server.ts",
    "dev:js": "cross-env NODE_MODE=development nodemon -r dotenv/config ./_ts-build/master-server.js",
    "dev:dc": "cross-env NODE_MODE=development DIRECT_COMMANDS=direct_commands nodemon -r dotenv/config",
    "prod": "cross-env NODE_MODE=production nodemon -r dotenv/config master-server.ts",
    "pm2:start:dev": "tsc && pm2 start pm2.config.js --name wings-server-controller --env development",
    "pm2:start:prod": "tsc && pm2 start pm2.config.js --name wings-server-controller --env production",
    "pm2:stop": "pm2 stop ./_ts-build/master-server.js --name wings-server-controller",
    "pm2:delete": "pm2 delete ./_ts-build/master-server.js --name wings-server-controller",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^7.0.3",
    "nodemon": "^2.0.20",
    "pm2": "^5.2.2",
    "serialport": "^10.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
